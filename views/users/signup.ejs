<form action="/users" method="POST">
  <div class="form-group">
    <label for="authEmail">Email</label>
    <input id="authEmail" class="form-control" type="email" name="email" value="seth@gmail.com">
  </div>

  <div class="form-group">
    <label for="authName">Name</label>
    <input id="authName" class="form-control" type="text" name="name" value="seth">
  </div>

  <div class="form-group">
    <label for="authPassword">Password</label>
    <input id="authPassword" class="form-control" type="password" name="password" value="abcde123456">
  </div>

  <input class="btn btn-primary" type="submit">
</form>
